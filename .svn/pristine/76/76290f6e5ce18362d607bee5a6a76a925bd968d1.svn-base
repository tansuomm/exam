#required {
	text-align:left;
}
#required #tab .tabline {
	width:500px;
}
#required .tabCont{
	border-left:#cacaca solid 1px;
	border-right:#cacaca solid 1px;
	padding-left:20px;
}
#required .foot {
	border:#cacaca solid 1px;
	border-top-style:none;
	padding:10px 20px;
}
#required .tabCont .block{

}
#required .tabCont .block .titleInfo {
	margin-left:35px;
	height:30px;
	padding-top:20px;
}
#required .tabCont .block .titleInfo img{
	position:relative;
	top:4px;
}
#required .tabCont .block .bottomInfo {
	text-align:left;
	padding-top:10px;
	margin-left:35px;
	height:30px;
	width:617px;
}
#required .tabCont .block .titleList {
	border-bottom:#999 solid 1px;
	font-weight:bold;
	text-align:center;
	line-height:30px;

	width:780px;
}
#required .tabCont .block .contList1 {
	text-align:center;
	line-height:30px;

	width:780px;
}
#required .tabCont .block .contList2 {
	text-align:center;
	line-height:30px;

	width:780px;
	background-color:#F1ECEC;
}
.titleInfo span {
	height:30px;
	display:block;
	line-height:30px;	
	float:left ;
}
span.name {
	width:170px;
	padding-top:7px;
	height:23px;
	padding-left:10px;
	text-align:left;
	float:left;
}
span.person {
	width:78px;
	float:left;
}
span.selecttime {
	width:85px;
	float:left;
}
span.time {
	width:84px;
	float:left;
}
span.cent {
	width:80px;
	float:left;
}
span.states {
	width:60px;
	float:left;
}
span.enterbtn {
	width:200px;
	padding-top:5px;
	height:25px;
	float:left;
	text-align :left;
}
#required .tabCont .block .titleList span.enterbtn {
	width:200px;
	padding:0;height:30px;
	float:left;
}
#required .tabCont .block .titleList span.name {
	width:180px;
	padding:0;
	height:30px;
	text-align:center;
	float:left;
}
#required .tabCont .block .line {
	width:720px;
	font-size:0px;
	height:10px;
	background:url(../../../App_Image/Learning/line_duan.gif) repeat-x;
}
