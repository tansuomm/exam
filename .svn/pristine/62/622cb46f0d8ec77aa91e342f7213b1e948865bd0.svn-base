#selectStudent {
	padding-left:20px;
}
#selectStudent .fieldsetClass{
	width:490px;
	margin:0;
	padding:0 10px;
	border:1px solid #C6BEC6;
}
#selectStudent .selectInfo {
	width:490px;
	height:30px;
}
#selectStudent .selectArea {
	width:510px;
}
#selectStudent .selectArea .readySelect{
	width:220px;
	float:left;
}
#selectStudent .selectArea .selectLabel {
	height:40px;
	line-height:40px;
	border:#999 solid 1px;
}
#selectStudent .selectArea .selectCont {
	height:295px;
	border:#999 solid 1px;
	border-top-style:none;
}
#selectStudent .selectArea .btnSelect{
	width:66px;
	float:left;
	text-align:center;
	border-left-style:none;
	border-right-style:none;
}
#selectStudent .selectArea .btnSelect .setright{
	margin-top:60px;
}
#selectStudent .selectArea .btnSelect .setleft{
	margin-top:80px;
}
#selectStudent .selectArea .alreadySelect{
	width:220px;
	float:left;
}
#selectStudent .btnsubmit {
	width:500px;
	margin-top:10px;
	text-align:center;
}