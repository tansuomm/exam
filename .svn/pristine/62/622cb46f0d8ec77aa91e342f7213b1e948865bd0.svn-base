body {
	margin:0 auto;
	text-align:left;
	font-size:12px;
}
.popTitle{/*弹出窗体的标题*/
color:#fff;
margin-bottom:5px;
font-weight:bold;
background:url(../../App_Image/Admin/xiao_title_back.gif) repeat-x;
height:24px;
line-height:22px;
text-indent:1em;
width:100%;
text-align:left;
}
.title {
	height:14px;
	padding-top:8px;
	text-align:center;
	font-weight:bold;
}
.content {
	text-align:left;
	padding:0 30px;
}
.four_button_y{ /*黄色按钮,4字*/
	background-image:url(../../App_Image/Admin/four_button_b.gif);
	height: 24px;
	width: 80px;
	border: 0px outset;
    font-family: "Verdana, Arial, sans-serif";
    cursor:hand;
	color:#000;
	line-height:29px;
}
.iconTitle {/*大icon的标题*/
	height:40px;
	line-height:40px;
	font-weight:bold;
}
.boldTitle {
	font-weight:bold;
}
.newsContTitle {/*有icon的新闻news*/
	height:20px;
	padding-left:50px;
}
.newsList {
	padding-left:20px;
}
.newsList .newsContTitle {
	height:26px;
}
.newsCont {/*无icon的新闻news*/
	padding-left:60px;
}
